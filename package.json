{
  "name": "demo-cli-markdown-migration",
  "version": "0.2.0",
  "description": "Simple bare-bones tool to generate Contentful import files from Markdown URLs and validate markdown quality.",
  "main": "src/generate-import-file.js",
  "scripts": {
    "test:generate": "node src/generate-import-file.js https://raw.githubusercontent.com/eric-schmidt/demo-cli-markdown-migration/refs/heads/main/markdown/markdown.md",
    "test:validate": "node src/validate.js https://raw.githubusercontent.com/eric-schmidt/demo-cli-markdown-migration/refs/heads/main/markdown/markdown-broken.md"
  },
  "keywords": [
    "contentful",
    "markdown",
    "migration",
    "import",
    "validation"
  ],
  "author": "Eric Schmidt",
  "license": "MIT",
  "dependencies": {
    "marked": "^11.0.0"
  }
}
